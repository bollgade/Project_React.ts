{
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"i18n-ally.enabledFrameworks": [
		"react",
		"i18next",
		"general"
	],
	"i18n-ally.localesPaths": [
		"public/locales",
		"src/shared/config/i18n"
	],
